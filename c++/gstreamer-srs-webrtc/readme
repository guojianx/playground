-pthread
-I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include/gstreamer-1.0
-I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include
-I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include/glib-2.0
-I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/lib/glib-2.0/include
-L/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/lib

-lgstreamer-1.0 -lgobject-2.0 -lglib-2.0  



gcc -O0 -g -Wall main.c -pthread -pthread -I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include/gstreamer-1.0 -I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include -I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/include/glib-2.0 -I/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/lib/glib-2.0/include -L/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/lib

PKG_CONFIG_PATH=/root/WebRTC/gstreamer/cerbero/build/dist/linux_x86_64/lib/pkgconfig
gcc -O0 -g -Wall main.c $(pkg-config --static --cflags --libs gstreamer-1.0)

